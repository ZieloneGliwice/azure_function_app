{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "cosmosDbRes",
      "type": "cosmosDB",
      "databaseName": "GreenGliwice",
      "collectionName": "Trees",
      "createIfNotExists": true,
      "partitionKey": "/userId",
      "connectionStringSetting": "CosmosDbConnectionString",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/CreateTree/index.js"
}
